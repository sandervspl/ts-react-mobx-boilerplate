    "mobx-react-router": "^4.0.5",
    "qs": "^6.5.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.0.2"
  },
