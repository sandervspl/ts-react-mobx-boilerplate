{
  "name": "ts-react-ssr-boilerplate",
  "version": "2.0.1",
  "description": "A starter boilerplate to create comprehensive React apps with TypeScript, Redux, Styled-Components and server-side rendering.",
  "author": "sandervispoel <contact@sandervispoel.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/sandervspl/ts-react-ssr-boilerplate"
  },
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development ts-node -r tsconfig-paths/register ./src/server/index.tsx",
    "build": "NODE_ENV=production run-p clean build:server build:client",
    "build:client": "webpack --hide-modules --config webpack/client-prod.ts",
    "build:server": "webpack --hide-modules --config webpack/server.ts",
    "start": "node dist/server.js",
    "clean": "rimraf dist/",
    "lint": "eslint src/"
  },
  "keywords": [
    "react",
    "webpack",
    "express",
    "redux",
    "boilerplate",
    "babel",
    "typescript",
    "styled-components",
    "server-side",
    "server side",
    "ssr"
  ],
  "dependencies": {
    "compression": "^1.7.2",
    "express": "^4.16.3",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.2.1",
    "svg-react-loader": "^0.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/polyfill": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0-beta.44",
    "@babel/preset-stage-0": "^7.0.0-beta.44",
    "@babel/preset-typescript": "^7.0.0-beta.44",
    "@types/express": "^4.11.1",
    "@types/lodash": "^4.14.106",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.3.11",
    "@types/react-dom": "^16.0.5",
    "@types/react-hot-loader": "^3.0.6",
    "@types/react-redux": "^5.0.15",
    "@types/react-router-dom": "^4.2.6",
    "@types/webpack": "^4.1.3",
    "@types/webpack-merge": "^4.1.3",
    "awesome-typescript-loader": "^5.0.0-1",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.1.0",
    "npm-run-all": "^4.1.1",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2",
    "ts-node": "^5.0.1",
    "tsconfig-paths": "^3.1.3",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.7.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.14",
    "webpack-dev-middleware": "^3.1.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0"
  },
  "engines": {
    "node": "8.5.0"
  }
}
